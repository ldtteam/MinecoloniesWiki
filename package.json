{
  "name": "minecolonies-wiki",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=23.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm run lint && astro build",
    "dev": "astro dev",
    "lint": "astro check && tsc --noEmit && pnpm run eslint",
    "lint:fix": "astro check && tsc --noEmit && pnpm run eslint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "preview": "astro preview",
    "start": "astro dev",
    "sync": "astro sync",
    "upgrade": "pnpm run upgrade:astro && pnpm run upgrade:deps",
    "upgrade:astro": "pnpm dlx @astrojs/upgrade",
    "upgrade:deps": "ncu -u -x sass && pnpm install"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/markdoc": "^0.12.7",
    "@astrojs/markdown-remark": "^6.0.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/svelte": "^7.0.4",
    "@fontsource/fondamento": "^5.1.1",
    "@fontsource/neucha": "^5.1.1",
    "@iconify-json/fa6-brands": "^1.2.5",
    "@iconify-json/fa6-regular": "^1.2.3",
    "@iconify-json/fa6-solid": "^1.2.3",
    "@iconify/json": "^2.2.300",
    "@markdoc/markdoc": "^0.4.0",
    "archiver": "^7.0.1",
    "astro": "^5.1.10",
    "astro-icon": "1.1.5",
    "astro-purgecss": "^5.1.0",
    "bootstrap": "^5.3.3",
    "date-fns": "^4.1.0",
    "iconify": "^1.4.0",
    "minecraft-data": "^3.83.1",
    "purgecss": "^7.0.2",
    "sharp": "^0.33.5",
    "svelte": "^5.19.4",
    "swiper": "^11.2.1",
    "vite": "^6.0.11"
  },
  "devDependencies": {
    "@astrojs/upgrade": "^0.4.3",
    "@eslint/js": "^9.19.0",
    "@types/archiver": "^6.0.3",
    "@types/eslint-plugin-markdown": "^2.0.2",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/shelljs": "^0.8.15",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-markdownlint": "^0.6.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "npm-check-updates": "^17.1.14",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.77.6",
    "shelljs": "^0.8.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "8.22.0"
  }
}
