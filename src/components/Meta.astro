---
interface Props {
  title: string;
}

const { title } = Astro.props;

const desc = 'Minecolonies is a town building mod that allows you to create your own thriving Colony within Minecraft!';

const keywords =
  'minecolonies,minecraft,minecraft mod,1.12,1.14,1.15,1.16,1.17,1.18,minecraft schematics,schematics minecraft,minecraft buildings,minecraft forge,minecraft server,minecraft mods,town building,minecraft colony,minecraft colonies,colony,colonies';
---

<meta charset="utf-8" />
<meta name="description" content={desc} />
<meta name="keywords" content={keywords} />
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
<meta name="theme-color" content="#4715A7" />
<meta name="msapplication-TileColor" content="#da532c" />

<meta name="og:title" content={title} />
<meta name="og:description" content={desc} />
<meta name="og:type" content="website" />
<meta name="og:url" content="https://wiki.minecolonies.com" />
<meta name="og:image" content="https://minecolonies.com/images/icon_townhallbook.png" />
